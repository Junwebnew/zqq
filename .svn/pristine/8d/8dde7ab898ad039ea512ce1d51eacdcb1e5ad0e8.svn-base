/* 咨询 */
.consultation>li {
  float: left;
  width: 32%;
  margin-left: 2%;
}

.consultation>li .firstBox {
  width: 100%;
  border: 1px solid #ddd;
  position: relative;
}

.consultation>li .firstBox a {
  display: block;
  width: 100%;
  height: 240px;
  overflow: hidden;
}

.consultation>li .firstBox p {
  margin: 10px 0;
  padding: 0 2px;
  text-align: left;
}

.consultation>li .firstBox .flag {
  position: absolute;
  left: -8px;
  top: 0;
  width: 70px;
  height: 24px;
  line-height: 24px;
  background: #fd7d22;
  color: #fff;
  z-index: 2;
  font-size: 12px;
  text-align: center;
}

.consultation>li .firstBox .flag:after {
  content: " ";
  position: absolute;
  left: 0px;
  bottom: -8px;
  width: 0;
  height: 0;
  border-top: 8px solid #9e4909;
  border-left: 8px solid transparent;
  border-right: 0px solid transparent;
  background-color: #f2f4f6;
}



.consultation>li img {
  width: 100%;
  height: 200px;
  transition: .2s all;
  -webkit-transition: .2s all;
  -moz-transition: .2s all;
  -ms-transition: .2s all;
  -o-transition: .2s all;
  transform: scale(1);
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  -o-transform: scale(1);
}

.consultation>li img:hover {
  transform: scale(1.05);
  -webkit-transform: scale(1.05);
  -moz-transform: scale(1.05);
  -ms-transform: scale(1.05);
  -o-transform: scale(1.05);
}

.consultation>li:first-child {
  margin-left: 0;
}

.consult_list>li {
  padding: 2px;
  color: #b8b8b8;
  position: relative;
  left: 0;
  transition: all .5s;
  -webkit-transition: all .5s;
