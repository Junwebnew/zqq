/* banner动画 */

.piao {
  position: absolute;
}

.piao1 {
  right: 140px;
  bottom: 0;
  width: 360px;
  height: 180px;
  overflow: hidden;
}

.piao1 img {
  -webkit-animation: pl1 60s linear infinite;
  -moz-animation: pl1 60s linear infinite;
  -ms-animation: pl1 60s linear infinite;
  animation: pl1 60s linear infinite;
}

@keyframes pl1 {
  from {
    transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -ms-transform: rotate(0deg);
    -o-transform: rotate(0deg);
  }

  50% {
    transform: rotate(180deg);
    -webkit-transform: rotate(180deg);
    -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
  }

  to {
    transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
    -moz-transform: rotate(360deg);
    -ms-transform: rotate(360deg);
    -o-transform: rotate(360deg);
  }
}

.piao2 {
  bottom: 10px;
  left: 10%;
  margin-left: 0px;
  -webkit-animation: pl2 20s linear infinite;
  -moz-animation: pl2 20s linear infinite;
  -ms-animation: pl2 20s linear infinite;
  animation: pl2 20s linear infinite;
}

@keyframes pl2 {
  from {
    opacity: 1;
    bottom: 10px;
  }

  to {
    bottom: 90%;
  }
}

@-webkit-keyframes pl2 {
  from {
    opacity: 1;
    bottom: 10px;
  }

  to {
    bottom: 90%;
  }
}

.piao3 {
  bottom: 10px;
  left: 20%;
  -webkit-animation: pl3 15s linear infinite;
  -moz-animation: pl3 15s linear infinite;
  -ms-animation: pl3 15s linear infinite;
  animation: pl3 15s linear infinite;
}


@-webkit-keyframes pl3 {
  from {
    bottom: 10px;
    opacity: 1;
  }

  15% {
    -webkit-transform: rotate(20deg) translateX(-10px);
    -moz-transform: rotate(20deg) translateX(-10px);
    -ms-transform: rotate(20deg) translateX(-10px);
    transform: rotate(20deg) translateX(-10px);
  }

  50% {
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
  }

  75% {
    -webkit-transform: rotate(-20deg) translateX(10px);
    -moz-transform: rotate(-20deg) translateX(10px);
    -ms-transform: rotate(-20deg) translateX(10px);
    transform: rotate(-20deg) translateX(10px);
  }

  90% {
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
    opacity: 1;
  }

  to {
    bottom: 90%;
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
    opacity: 0;
  }
}


@keyframes pl3 {
  from {
    bottom: 10px;
    opacity: 1;
  }

  15% {
    -webkit-transform: rotate(20deg) translateX(-10px);
    -moz-transform: rotate(20deg) translateX(-10px);
    -ms-transform: rotate(20deg) translateX(-10px);
    transform: rotate(20deg) translateX(-10px);
  }

  50% {
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
  }

  75% {
    -webkit-transform: rotate(-20deg) translateX(10px);
    -moz-transform: rotate(-20deg) translateX(10px);
    -ms-transform: rotate(-20deg) translateX(10px);
    transform: rotate(-20deg) translateX(10px);
  }

  90% {
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
    opacity: 1;
  }

  to {
    bottom: 90%;
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
    opacity: 0;
  }
}

