    -ms-transform: rotate(-20deg) translateX(10px);
    transform: rotate(-20deg) translateX(10px);
  }

  90% {
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
    opacity: 1;
  }

  to {
    bottom: 90%;
    -webkit-transform: rotate(0deg) translateX(0);
    -moz-transform: rotate(0deg) translateX(0);
    -ms-transform: rotate(0deg) translateX(0);
    transform: rotate(0deg) translateX(0);
    opacity: 0;
  }
}

.piao4 {
  bottom: 10px;
  left: 50%;
  margin-left: 0px;
  -webkit-animation: pl4 20s linear infinite;
  -moz-animation: pl4 20s linear infinite;
  -ms-animation: pl4 20s linear infinite;
  animation: pl4 20s linear infinite;
}

@keyframes pl4 {
  from {
    bottom: 10px;
  }

  25% {
    left: 53%;
  }

  50% {
    left: 50%;
  }

  75% {
    left: 47%;
  }

  to {
    opacity: .6;
    left: 50%;
    bottom: 90%;
  }
}

@-webkit-keyframes pl4 {
  from {
    bottom: 10px;
  }

  25% {
    left: 53%;
  }

  50% {
    left: 50%;
  }

  75% {
