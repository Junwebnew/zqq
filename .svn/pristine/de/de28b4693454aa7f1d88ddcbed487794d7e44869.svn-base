/*时间轴*/
.patentDetail-wrap .time-axis {
  display: none;
  position: fixed;
  _position: absolute;
  right: 0%;
  top: 32%;
  z-index: 1;
}

.patentDetail-wrap .time-axis:before {
  content: "";
  position: absolute;
  left: 4px;
  width: 2px;
  height: 100%;
  background-color: #DDDDDD;
}

.patentDetail-wrap .time-axis .time-axis-header {
  padding: 14px 28px;
  position: relative;
}

.patentDetail-wrap .time-axis .bg-top {
  display: block;
  position: absolute;
  left: 0px;
  top: -7px;
  width: 10px;
  height: 10px;
  border: 2px solid #ddd;
  background-color: #fff;
  z-index: 1;
  border-radius: 50%;
}

.patentDetail-wrap .time-axis .bg-bottom {
  display: block;
  position: absolute;
  left: 0px;
  bottom: -7px;
  width: 10px;
  height: 10px;
  border: 2px solid #ddd;
  background-color: #fff;
  z-index: 1;
  border-radius: 50%;
}

.patentDetail-wrap .time-axis .time-axis-header .icon-disc {
  display: block;
  position: absolute;
  left: 0px;
  top: 50%;
  margin-top: -5px;
  background: #cccccc;
  border: 2px solid #ffffff;
  width: 10px;
  height: 10px;
  border-radius: 100%;
}

.patentDetail-wrap .time-axis .time-axis-header .text {
  display: block;
  float: left;
  font-size: 14px;
  color: #333;
  position: relative;
  z-index: 2;
}

.patentDetail-wrap .time-axis .time-axis-header .tipsMsg {
  line-height: 26px;
  padding: 0 10px;
  background-color: #f08b30;
  color: #fff;
  border-radius: 2px;
  position: absolute;
  right: 190px;
  top: 50%;
  margin-top: -18px;
  font-size: 12px;
  white-space: nowrap;
  opacity: 0;
  visibility: hidden;
  -webkit-transition: 0.3s;
  -moz-transition: 0.3s;
  -ms-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
  -webkit-transform: translate(0, 4px);
  -moz-transform: translate(0, 4px);
  -ms-transform: translate(0, 4px);
  -o-transform: translate(0, 4px);
  transform: translate(-100px, 5px);
}

.patentDetail-wrap .time-axis .time-axis-header .tipsMsg .icon-arrow {
  display: block;
  position: absolute;
  right: -12px;
  top: 50%;
  width: 0;
  height: 0;
  border: 4px solid transparent;
  margin-top: -4px;
  border-left: 8px solid #f08b30;
}

.patentDetail-wrap .time-axis .time-axis-header.active .icon-disc {}

.patentDetail-wrap .time-axis .time-axis-header.active .text {
  color: #F08B2F;
}

.patentDetail-wrap .time-axis .time-axis-header.active .tipsMsg {
  opacity: 1;
  visibility: visible;
  -webkit-transform: translate(0, 4px);
  -moz-transform: translate(0, 4px);
  -ms-transform: translate(0, 4px);
  -o-transform: translate(0, 4px);
  transform: translate(00px, 5px);
}

.time-axis-arrow {
  position: absolute;
  left: -2px;
  top: 17px;
  width: 20px;
  height: 12px;
  background: url(../images/main/icon-scroll-arrow.png) no-repeat center;
  z-index: 3;
  -webkit-transition: 0.3s;
  -moz-transition: 0.3s;
  -ms-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
}
