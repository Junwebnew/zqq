@charset "utf-8";

/** ---------- 此css文件是专利搜索的样式 ---------- **/
.baseFontSize12 {
  font-size: 12px;
}

.baseFontSize14 {
  font-size: 14px;
}

.baseFontSize16 {
  font-size: 16px;
}

.baseFontColor {
  color: #f08b30;
}

.baseFontColor1 {
  color: #777;
}

.baseBorderColor {
  border-color: #eee;
}

.text-overflow {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

/* ---------- start 大搜索框页 ---------- */
.patentSearchList-wrap {
  margin: auto;
  padding-top: 30px;
}

.patentSearchList-wrap .patent-search-Maintitle {
  float: left;
  width: 200px;
  height: 100px;
}

.patentSearchList-wrap .patent-searchBox-container {
  margin-left: 20px;
}

.patentSearchList-wrap .patent-searchBox {
  margin-bottom: 10px;
  text-align: center;
  width: 100%;
}

.patentSearchList-wrap .patent-searchBox .qds-search-common {
  float: left;
  position: relative;
}

.patentSearchList-wrap .patent-searchBox .qds-search-common .search-text {
  position: relative;
  float: left;
  width: 725px;
  height: 50px;
  line-height: 50px;
  padding: 0 15px;
  border: solid 1px #f0f0f0;
  font-size: 16px;
  color: #999;
  background-color: #fff;
  width: 740px;
  border: solid 1px #f08c2f;
  color: #777;
}

.patentSearchList-wrap .patent-searchBox .qds-search-common .search-text::-webkit-input-placeholder {
  font-size: 16px;
  color: #999999;
  font-family: "Microsoft Yahei", "微软雅黑";
}

.qds-search-common .search-btn {
  float: left;
  border: none;
  width: 140px;
  height: 50px;
  color: #fff;
  font-size: 24px;
  cursor: pointer;
  border-radius: 0 4px 4px 0;
  -webkit-border-radius: 0 4px 4px 0;
  -moz-border-radius: 0 4px 4px 0;
  -ms-border-radius: 0 4px 4px 0;
  -o-border-radius: 0 4px 4px 0;
}

.patentSearchList-wrap .patent-searchBox .qds-search-common .clearAll {
  width: 20px;
  height: 20px;
  line-height: 18px;
  font-size: 20px;
  position: absolute;
  top: 14px;
  right: 150px;
  color: #bbb;
  text-align: center;
}

.patentSearchList-wrap .patent-searchBox .qds-search-common .clearAll:hover {
  color: #f08b30;
}

.patentSearchList-wrap .patent-searchBox .help {
  float: left;
  padding-right: 30px;
  position: relative;
  margin: 0 0 0 25px;
  line-height: 50px;
}

.patentSearchList-wrap .patent-searchBox .help-tips {
  display: none;
  position: absolute;
  left: 80px;
  top: -50px;
  padding: 15px;
  width: 300px;
  text-align: justify;
  line-height: 1.2em;
  color: #888;
  background-color: #fff;
  border: 1px solid #ddd;
  border-radius: 5px;
}

.patentSearchList-wrap .patent-searchBox .keyword-box {
  position: absolute;
  left: 12px;
  top: 12px;
  max-width: 925px;
  height: 28px;
  overflow: hidden;
}

.patentSearchList-wrap .patent-searchBox .keyword-box .list {
  float: left;
  background-color: #fff;
  line-height: 24px;
  border: solid 1px #e9e9e9;
  padding: 0 8px;
  cursor: default;
  margin: 0 14px 4px 0;
  position: relative;
}

.patentSearchList-wrap .patent-searchBox .keyword-box .list .text {
  display: block;
  float: left;
  font-size: 12px;
  color: #777;
  max-width: 125px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.patentSearchList-wrap .patent-searchBox .keyword-box .list .icon-close {
  float: left;
  width: 14px;
  height: 14px;
  line-height: 14px;
  margin: 4px 0 0 6px;
  text-align: right;
  background: none;
  cursor: pointer;
  font-size: 20px;
  color: #bbb;
  overflow: hidden;
  font-family: "Microsoft Yahei", "微软雅黑";
}

.patentSearchList-wrap .patent-searchBox .keyword-box .list:hover .icon-close {
  color: #f08b30;
}

.patentSearchList-wrap .patent-searchBox .keyword-box .list .tipsMsg {
  display: none;
  border: solid 1px #e9e9e9;
  border-radius: 4px;
  z-index: 99;
  position: absolute;
  top: 34px;
  left: 0px;
  padding: 12px 15px;
  color: #777;
  background-color: #fff;
  font-size: 12px;
  white-space: nowrap;
  box-shadow: 0 0 12px #ddd;
}

.patentSearchList-wrap .patent-searchBox .keyword-box .list .tipsMsg .icon-arrow {
  position: absolute;
  top: -8px;
  left: 20px;
  font-size: 20px;
  color: #e9e9e9;
  background-color: #fff;
  width: 8px;
  height: 9px;
  line-height: 15px;
  overflow: hidden;
  text-indent: -6px;
}

/* ---------- end 大搜索框页 ---------- */

/* ---------- start 搜索框下面的5个条件 ---------- */
.icon {
  background-position: 0px 0px;
  background-repeat: no-repeat;
  display: block;
}

.icon-checkbox-patent {
  background-image: url('../images/main/icon-patent.png');
  background-position: 0px 0px;
}


.patentSearchList-wrap .patent-condition {
  margin-top: 10px;
  padding-left: 16px;
}

.patentSearchList-wrap .patent-condition .list {
  display: block;
  float: left;
  margin: 0 22px 0 0;
  font-size: 13px;
  color: #333;
  cursor: pointer;
}

.patentSearchList-wrap .patent-condition .list .icon-checkbox-patent {
  display: block;
  float: left;
  width: 13px;
  height: 13px;
  margin: 3px 5px 0 0;
}

.patentSearchList-wrap .patent-condition .list.selected .icon-checkbox-patent {
  background-position: -15px 0px;
}

.patentSearchList-wrap .patent-condition .list.selected {
  color: #f08c2f;
}

/* ---------- start 关联词 ---------- */
.patentSearchList-wrap .related-words {
  margin-top: 25px;
  font-size: 12px;
  color: #777;
}

.patentSearchList-wrap .related-words dl dt {
  float: left;
  width: 55px;
  line-height: 32px;
}

.patentSearchList-wrap .related-words dl dd {
  float: left;
  width: 1180px;
  max-height: 104px;
  overflow: hidden;
  position: relative;
}

.patentSearchList-wrap .related-words dl dd .word-list {
  margin-bottom: 6px;
  height: 28px;
  overflow: hidden;
  padding: 5px;
}

.patentSearchList-wrap .related-words dl dd .word-list .word-title {
  display: block;
  float: left;
  width: 65px;
  line-height: 25px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: bold;
}

.patentSearchList-wrap .related-words dl dd .word-list .word-content {
  float: left;
  width: 950px;
}

.patentSearchList-wrap .related-words dl dd .word-list .word-content li {
  float: left;
  height: 22px;
  line-height: 18px;
  margin: 3px 12px 3px 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.patentSearchList-wrap .related-words dl dd .word-list .word-content li a {
  display: inline-block;
  padding: 0 6px;
  color: #777;
  background-color: #f6f6f6;
  border: 1px solid #F6F6F6;
}

.patentSearchList-wrap .related-words dl dd .word-list .word-content li a:hover {
  border: 1px solid #F08B2F;
  color: #F08B2F;
  background-color: #fff;
  border-radius: 2px;
}

.patentSearchList-wrap .related-words dl dd .word-list .word-content li a.selected {
  border: 1px solid #F08B2F;
  color: #F08B2F;
  background-color: #fff;
  border-radius: 2px;
}

.patentSearchList-wrap .related-words dl dd .show-more {
  display: block;
  padding-left: 15px;
  color: #777;
  position: absolute;
  right: 0;
  top: 10px;
}

.patentSearchList-wrap .related-words dl dd .show-more .icon-triangle-up {
  position: absolute;
  left: 0;
  top: 4px;
  display: block;
  width: 0;
  height: 0;
  border: 5px solid transparent;
  border-top: 6px solid #f08b30;
  -webkit-transition: 0.3s;
  -moz-transition: 0.3s;
  -ms-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
}

.patentSearchList-wrap .related-words dl dd.open {
  max-height: none;
}

.patentSearchList-wrap .related-words dl dd.open .show-more .icon-triangle-up {
  -webkit-transform: rotate(180deg) translate(0, 4px);
  -moz-transform: rotate(180deg) translate(0, 4px);
  -ms-transform: rotate(180deg) translate(0, 4px);
  -o-transform: rotate(180deg) translate(0, 4px);
  transform: rotate(180deg) translate(0, 5px);
}

/* ---------- start 筛选 ---------- */
.patentSearchList-wrap .query-condition .list-box {
  margin-top: 25px;
}

.patentSearchList-wrap .query-condition .list-box .list {
  display: block;
  float: left;
  height: 24px;
  line-height: 22px;
  font-size: 12px;
  color: #aaa;
  font-family: "宋体", SimSun;
  overflow: hidden;
}

.patentSearchList-wrap .query-condition .list-box .list.arrow {
  margin: 0px 8px;
}

.patentSearchList-wrap .query-condition .list-box .list a {
  color: #aaa;
}

.patentSearchList-wrap .query-condition .list-box .list .text-border {
  display: block;
  height: 24px;
  padding: 0 8px;
  margin-right: 8px;
  border: dashed 1px #e4e2e0;
  color: #aaa;
}

.patentSearchList-wrap .query-condition .list-box .list .text-border .text {
  display: block;
  float: left;
}

.patentSearchList-wrap .query-condition .list-box .list .text-border .icon-close {
  float: right;
  width: 9px;
  height: 9px;
  margin: 6px 0 0 6px;
  cursor: pointer;
  background-color: #fff;
}

.patentSearchList-wrap .query-condition .list-box .list .text-border:hover {
  border: dashed 1px #f08c2f;
  color: #f08c2f;
}

.patentSearchList-wrap .query-condition .list-box .list .text-border:hover .icon-close {
  background-position: -13px -13px;
}

/* ---------- start 排序条件 ---------- */
.patentSearchList-wrap .sort-condition {
  margin-top: 15px;
  height: 40px;
  padding: 0 20px;
  position: relative;
  font-size: 12px;
  color: #777;
  background-color: #eee;
}

.patentSearchList-wrap .sort-condition .s-left {
  line-height: 40px;
}

.patentSearchList-wrap .sort-condition .s-right {
  float: right;
  line-height: 40px;
}

.patentSearchList-wrap .sort-condition .s-right-dl {
  margin-left: 40px;
}

.patentSearchList-wrap .sort-condition dl {
  float: left;
}

.patentSearchList-wrap .sort-condition dl a {
  color: #777;
}

.patentSearchList-wrap .sort-condition dl .selected {
  color: #f08b30;
}

.patentSearchList-wrap .sort-condition dl dt {
  float: left;
}

.patentSearchList-wrap .sort-condition dl dd {
  float: left;
}

.patentSearchList-wrap .sort-condition dl .downup {
  display: block;
  margin: 13px 0 0 0;
  width: 20px;
  height: 16px;
  position: relative;
}

.patentSearchList-wrap .sort-condition dl .downup .icon {
  display: block;
  position: absolute;
  left: 5px;
  width: 0;
  height: 0;
  border: 5px solid transparent;
}

.patentSearchList-wrap .sort-condition dl .downup .icon-sort-up {
  top: -5px;
  border-bottom: 6px solid #c4c4c4;
}

.patentSearchList-wrap .sort-condition dl .downup .icon-sort-down {
  bottom: -4px;
  border-top: 6px solid #c4c4c4;
}

.patentSearchList-wrap .sort-condition dl .downup .icon-sort-up.icon-selected {
  border-bottom: 6px solid #f08b30;
}

.patentSearchList-wrap .sort-condition dl .downup .icon-sort-down.icon-selected {
  border-top: 6px solid #f08b30;
}

/*选择价格区间*/
.patentSearchList-wrap .sort-condition .select-price {
  position: relative;
  color: #999;
  font-size: 12px;
  margin-top: 8px;
}

.patentSearchList-wrap .sort-condition .select-price .select-title {
  display: block;
  width: 100px;
  height: 25px;
  line-height: 25px;
  border: solid 1px #e6e6e6;
  background-color: #fff;
  text-align: center;
}

.patentSearchList-wrap .sort-condition .select-price .select-title .text {
  display: inline-block;
  margin: 0 6px 0 0;
}

.patentSearchList-wrap .sort-condition .select-price .select-title .icon {
  display: inline-block;
  margin: 10px 0 0 0;
  width: 11px;
  height: 6px;
}

.patentSearchList-wrap .sort-condition .select-price .select-body {
  opacity: 0;
  visibility: hidden;
  overflow: hidden;
  position: absolute;
  z-index: 2;
  width: 100%;
  top: 25px;
  left: 0px;
  background-color: #fff;
  border: solid 1px #e6e6e6;
  border-top: 0;
  -webkit-transition: 0.3s;
  -moz-transition: 0.3s;
  -ms-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;

  -webkit-transform: translate(0, -10px);
  -moz-transform: translate(0, -10px);
  -ms-transform: translate(0, -10px);
  -o-transform: translate(0, -10px);
  transform: translate(0, -10px);
}

.patentSearchList-wrap .sort-condition .select-price .select-body .item {
  display: block;
  padding-left: 18px;
  height: 25px;
  line-height: 25px;
  margin-top: 0px;
  text-align: left;
}

.patentSearchList-wrap .sort-condition .select-price .select-body .item:hover {
  background-color: #F08B2F;
  color: #fff;
}

.patentSearchList-wrap .sort-condition .select-price .select-body .item.selected {
  background-color: #F08B2F;
  color: #fff;
}

.patentSearchList-wrap .sort-condition .select-price.open .select-body {
  opacity: 1;
  visibility: visible;
  -webkit-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
  -ms-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  transform: translate(0, 0);
}

/*输入价格区间*/
.patentSearchList-wrap .sort-condition .fill-price {
  margin: 0 0 0 10px;
}

.patentSearchList-wrap .sort-condition .fill-price label {
  display: block;
  float: left;
  position: relative;
}

.patentSearchList-wrap .sort-condition .fill-price .gang {
  display: block;
  float: left;
  margin: 0 5px;
}

.icon-arrow-down {
  background-image: url('../images/main/icon-arrow-down.png');
}

.patentSearchList-wrap .sort-condition .fill-price label .icon {
  display: block;
  width: 12px;
  height: 12px;
  position: absolute;
  right: 5px;
  top: 6px;
}

.patentSearchList-wrap .sort-condition .fill-price label input {
  background-color: #fff;
  border: solid 1px #e6e6e6;
  height: 25px;
  width: 80px;
  font-size: 12px;
  color: #999;
  text-align: center;
}

.patentSearchList-wrap .sort-condition .fill-price label:hover .icon {
  background-position: -13px -15px;
}

.patentSearchList-wrap .sort-condition .btn {
  margin: 0 0 0 10px;
}

.patentSearchList-wrap .sort-condition .btn .queding {
  display: block;
  margin-top: 9px;
  padding: 0 10px;
  line-height: 22px;
  color: #F08B2F;
  background-color: #fff;
  border-radius: 3px;
  border: solid 1px #e6e6e6;
}

.patentSearchList-wrap .sort-condition .btn .clear {
  display: block;
  margin-top: 9px;
  padding: 0 10px;
  line-height: 22px;
  background-color: #fff;
  border-radius: 3px;
  border: solid 1px #e6e6e6;
  margin-left: 5px;
}

/* ---------- start 结果列表展示 ---------- */
.patentSearchList-wrap .result-wrap .list {
  padding: 15px 10px;
  border-bottom: solid 1px #eee;
}

.patentSearchList-wrap .result-wrap .list .row-1 {
  padding: 6px 0;
}

.patentSearchList-wrap .result-wrap .list .row-1 .type {
  display: block;
  float: left;
  color: #666;
  font-size: 13px;
  margin-top: 2px;
}

.patentSearchList-wrap .result-wrap .list .row-1 .title {
  display: block;
  float: left;
  font-size: 15px;
  margin-left: 10px;
  max-width: 1000px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.patentSearchList-wrap .result-wrap .list .row-1 .title a {
  color: #666;
  font-weight: bold;
}

.patentSearchList-wrap .result-wrap .list .row-1 .title a:hover {
  color: #f08b30;
}

.patentSearchList-wrap .result-wrap .list .row-1 .word {
  color: #f08b30;
}

.law-state {
  display: block;
  float: left;
  color: #fff;
  font-size: 10px;
  padding: 0 5px;
  line-height: 20px;
  margin: 0px 0 0 12px;
  border-radius: 1px;
}

.law-state.bgcolor-1 {
  background-color: #a7df6a;
}

.law-state.bgcolor-2 {
  background-color: #c3c3c3;
}

.law-state.bgcolor-3 {
  background-color: #77ddc6;
}

.law-state.bgcolor-4 {
  background-color: #c88bd5;
}

.law-state.bgcolor-5 {
  background-color: #e27885;
}

.law-state.bgcolor-6 {
  background-color: #7bc3ec;
}

.law-state.bgcolor-7 {
  background-color: #f7c548;
}

.law-state.bgcolor-8 {
  background-color: #eaa262;
}

.patentSearchList-wrap .result-wrap .list .row-1 .related {
  display: block;
  float: right;
  color: #777;
  font-size: 12px;
  margin: 2px 0 0;
}

.patentSearchList-wrap .result-wrap .list .row-1 .related .icon-related {
  width: 14px;
  height: 14px;
  float: left;
  margin-top: 2px;
  margin-right: 3px;
  /* background-image: url(../images/icon-relate-gray.png); */
  background-repeat: no-repeat;
}

.patentSearchList-wrap .result-wrap .list .row-1 .related:hover {
  color: #f08b30;
}

.patentSearchList-wrap .result-wrap .list .row-1 .related:hover .icon-related {
  /* background-image: url(../images/icon-relate.png); */
  background-size: 12px;
}

.patentSearchList-wrap .result-wrap .list .row-2 {
  padding: 5px 0 6px;
  ;
}

.patentSearchList-wrap .result-wrap .list .row-2 a {
  display: block;
  float: left;
  color: #111;
  font-size: 13px;
  margin-right: 72px;
}

.patentSearchList-wrap .result-wrap .list .row-2 .main-classification-number {
  max-width: 212px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.patentSearchList-wrap .result-wrap .list .row-2 a:hover {
  text-decoration: underline;
}

.patentSearchList-wrap .result-wrap .list .row-3 {
  margin-top: 6px;
  padding: 3px 0 6px;
}

.patentSearchList-wrap .result-wrap .list .row-3 .profiles {
  display: block;
  font-size: 12px;
}

.patentSearchList-wrap .result-wrap .list .row-3 .profiles.open {
  height: auto;
}

.patentSearchList-wrap .result-wrap .list .row-3 .profiles .brief {
  color: #444;
  line-height: 20px;
}

.patentSearchList-wrap .result-wrap .list .row-3 .profiles .show-more {
  color: #f08b30;
  cursor: pointer;
  display: inline-block;
  margin-left: 6px;
}

.patentSearchList-wrap .result-wrap .list .row-4 {
  padding: 6px 0;
  font-size: 12px;
  line-height: 22px;
}

.patentSearchList-wrap .result-wrap .list .row-4 .title {
  display: block;
  float: left;
  color: #777;
  margin-right: 8px;
}

.patentSearchList-wrap .result-wrap .list .row-4 .word {
  display: block;
  float: left;
  color: #777;
  margin-right: 12px;
  background-color: #eee;
  padding: 0 6px;
  border: 1px solid #eee;
}

.patentSearchList-wrap .result-wrap .list .row-4 .word:hover {
  background-color: #fff;
  color: #F08B2F;
  border: 1px solid #F08B2F;
  border-radius: 2px;
}

.patentSearchList-wrap .result-wrap .list .row-4 .word.selected {
  background-color: #fff;
  color: #F08B2F;
  border: 1px solid #F08B2F;
  border-radius: 2px;
}

.patentSearchList-wrap .result-wrap-shadow {
  margin: 32px 0;
  border: solid 1px #eee;
  box-shadow: 0px 0px 10px #e0e0e0;
  position: relative;
}

.patentSearchList-wrap .result-wrap-shadow .icon-bg {
  display: block;
  width: 65px;
  height: 72px;
  /* background: url("../images/icon-patent.png") no-repeat -46px 0px; */
  position: absolute;
  bottom: 0;
  right: 5px;
}

.patentSearchList-wrap .result-wrap-shadow .list {
  padding: 18px 25px;
}

/* ---------- start 分页 ---------- */
.patentSearchList-wrap .mypaging {
  padding: 55px 0;
}


/* ---------- start 专利详情 ---------- */
.patentDetail-wrap {
  margin: auto;
}

/* ---------- start 标题 ---------- */
.patentDetail-wrap .page-title {
  padding-top: 35px;
}

.patentDetail-wrap .page-title .type {
  display: block;
  float: left;
  margin-top: 2px;
  color: #666;
  font-size: 14px;
}

.patentDetail-wrap .page-title .title {
  display: block;
  float: left;
  font-size: 18px;
  margin-left: 10px;
  max-width: 800px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.patentDetail-wrap .page-title .title a {
