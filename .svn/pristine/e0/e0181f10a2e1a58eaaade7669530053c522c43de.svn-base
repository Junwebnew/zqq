<script>
import mainHead from "~/components/head/mainHead";
import fixNav from "~/components/fixNav";
import minNav from "~/components/minNav";
import floor from "~/components/floor/floor";
import { openNewPage } from "~/assets/js/common.js";
import { Http } from "~/plugins/axios.js";
import popup from "~/components/popup";

export default {
    layout: "foot",
    data() {
        return {
            navArr: ["商标", "专利", "企业"],
            holderArr: [
                "请输入商标名称、申请号、申请人等信息",
                "请输入专利名称",
                "请输入企业名称"
            ],
            keyword: "",
            styleIdx: 0,
            aiTab: ["商标智能注册系统", "商标快速报件系统", "商标分类表"],
            aiTabIdx: 0,
            isShow: 0,
            brandNum: 1000, //商标数据
            dimension: 0, //维度
            transactioNum: 0, //商标交易数据
            userNum: 0, //用户数据
            totalData: 0,
            totalUser: 0
        };
    },
    components: {
        mainHead,
        minNav,
        fixNav,
        floor
    },
    mounted() {
        //监听滚动条
        window.addEventListener("scroll", this.handleScroll);
        this.animatNum(45685236, "totalData");
        this.animatNum(214565, "totalUser");
        // let loading = popup.created({
        //     type: "loading",
        //     icon: "&#xe61c",
        //     content: "加载中...",
        //     time: 3000
        // });
        // setTimeout(() => {
        //     loading.close()
        // }, 3000)
        console.log("用户信息", this.$store.state);
    },
    methods: {
        // 格式化数字
        formatNum(num) {
            var num = (num || 0).toString();
            var result = "";
            while (num.length > 3) {
                result = "," + num.slice(-3) + result;
                num = num.slice(0, num.length - 3);
            }
            if (num) {
                result = num + result;
            }
            return result;
        },
        // 数字加载动画
        animatNum(num, type) {
            let total = 0;
            let time = setInterval(() => {
                if (total >= num) {
                    total = num;
                    clearInterval(time);
                } else {
                    total += parseInt(num / 200);
                }
                this[type] = this.formatNum(total);
            }, 20);
        },
        switchStyle(idx) {
            this.styleIdx = idx;
        },
        showMinNav(num) {
            this.isShow = num;
        },
        handleScroll() {
            var scrollTop =
                window.pageYOffset ||
                document.documentElement.scrollTop ||
                document.body.scrollTop;
            var offsetTop =
                document.getElementById("count").offsetTop -
                document.documentElement.clientHeight;
            if (scrollTop > offsetTop) {
                window.removeEventListener("scroll", this.handleScroll);
                let timer = setInterval(() => {
                    if (this.dimension < 360)
                        this.dimension = this.dimension + 2;
                    if (this.transactioNum < 100)
                        this.transactioNum = this.transactioNum + 1;
                    if (this.userNum < 20) this.userNum = this.userNum + 1;
                    if (this.brandNum < 3000) this.brandNum = this.brandNum + 8;
                    if (
                        this.dimension >= 360 &&
                        this.transactioNum >= 100 &&
                        this.userNum >= 20 &&
                        this.brandNum >= 3000
                    ) {
                        clearInterval(timer);
                    }
                }, 20);
            }
        },
        goSearch() {
            //去搜索页面
            if (!this.keyword) {
                openNewPage(this, "/search/searchPage");
                return;
            }
            if (this.styleIdx === 0)
                this.$router.push({
                    path: "/search/result." + this.keyword + ".html"
                });
            else if (this.styleIdx === 1)
                this.$router.push({
                    path: "/patentSearch/"
                });
            else if (this.styleIdx === 2) {
                window.open(
                    "https://dqc.dgg.net/#/Company?type=all&keyWord=" +
                        this.keyword,
                    "_blank"
                );
            }
        }
    },
    destroyed() {
        window.removeEventListener("scroll", this.handleScroll);
    }
};
</script>

