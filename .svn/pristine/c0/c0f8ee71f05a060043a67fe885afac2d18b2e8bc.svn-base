    methods: {
        anmime() {
            let timer = setInterval(() => {
                let score = this.dataScore;
                if (this.score == 0) {
                    this.msg = this.dataCause;
                    clearInterval(timer);
                }
                if (score == this.score) {
                    if (score < 90) this.msg = this.dataCause;
                    clearInterval(timer);
                } else {
                    this.score -= 1;
                }
            }, 50);
