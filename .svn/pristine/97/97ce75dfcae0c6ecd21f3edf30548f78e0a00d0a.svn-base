                time: 2000
            });
            window.location.href = "/show/navigation_trademark_register_04&S7741102214293331969.html";
        }
        if (this.$store.state.userInfo.userId) {
            this.loadDomain();
            this.loadType();
            this.loadApplicantName()
            this.loadTemplateUrl()
        }
    },
